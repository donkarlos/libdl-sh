quote_command@{:@@<:@3@:>@rm,-f,!@:}@=0=>rm<SP>-f<SP>'!'
quote_command@{:@@<:@3@:>@rm,-f,"@:}@=0=>rm<SP>-f<SP>'"'
quote_command@{:@@<:@3@:>@rm,-f,#@:}@=0=>rm<SP>-f<SP>'#'
quote_command@{:@@<:@3@:>@rm,-f,$@:}@=0=>rm<SP>-f<SP>'$'
quote_command@{:@@<:@3@:>@rm,-f,&@:}@=0=>rm<SP>-f<SP>'&'
quote_command@{:@@<:@3@:>@rm,-f,'@:}@=0=>rm<SP>-f<SP>''\'''
quote_command@{:@@<:@3@:>@rm,-f,@{:@@:}@=0=>rm<SP>-f<SP>'@{:@'
quote_command@{:@@<:@3@:>@rm,-f,@:}@@:}@=0=>rm<SP>-f<SP>'@:}@'
quote_command@{:@@<:@3@:>@rm,-f,*@:}@=0=>rm<SP>-f<SP>'*'
quote_command@{:@@<:@3@:>@rm,-f,+@:}@=0=>rm<SP>-f<SP>+
quote_command@{:@@<:@3@:>@rm,-f,.@:}@=0=>rm<SP>-f<SP>.
quote_command@{:@@<:@3@:>@rm,-f,;@:}@=0=>rm<SP>-f<SP>';'
quote_command@{:@@<:@3@:>@rm,-f,</dev/null@:}@=0=>rm<SP>-f<SP></dev/null
quote_command@{:@@<:@3@:>@rm,-f,2>/dev/null@:}@=0=>rm<SP>-f<SP>2>/dev/null
quote_command@{:@@<:@3@:>@rm,-f,?@:}@=0=>rm<SP>-f<SP>'?'
quote_command@{:@@<:@3@:>@rm,-f,@<:@@:}@=0=>rm<SP>-f<SP>'@<:@'
quote_command@{:@@<:@3@:>@rm,-f,\@:}@=0=>rm<SP>-f<SP>'\'
quote_command@{:@@<:@3@:>@rm,-f,@:>@@:}@=0=>rm<SP>-f<SP>'@:>@'
quote_command@{:@@<:@3@:>@rm,-f,^@:}@=0=>rm<SP>-f<SP>^
