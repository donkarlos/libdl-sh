dnl
dnl Check if quote() works as expected, i.e.
dnl   -	'quote sh QUOTEE' produces the same output as
dnl	'quote_sh QUOTEE',
dnl   -	'quote re [DELIMITER] QUOTEE' produces the same output as
dnl	'quote_re [DELIMITER] QUOTEE',
dnl   -	'quote ere [DELIMITER] QUOTEE' produces the same output as
dnl	'quote_ere [DELIMITER] QUOTEE',
dnl   -	'quote rs [DELIMITER] QUOTEE' produces the same output as
dnl	'quote_rs [DELIMITER] QUOTEE', and
dnl   -	'quote QUOTEE' behaves like 'quote sh QUOTEE' (i.e., that 'sh' is the
dnl	default quoting operator).
dnl
dnl NOTE:   (1)	The reference output is peppered with quadrigraphs because
dnl		it's as unbalanced as can be!
dnl
AT_SETUP([quote() works as expected? (be patient...)])
AT_KEYWORDS([libquote.sh libquote quote])
AT_CHECK([dnl
set -eu					## Employ strict execution environment
					## to ensure 'set -u' resilience.
case :$PATH: in				## Triggered by 'check-local:'?
    *:$abs_top_builddir/bin:*)		## Yes, export suitable LSP.
	LIBDL_SH_LIBRARY_PATH=$abs_top_builddir/lib
	LIBDL_SH_LIBEXECDIR=$abs_top_builddir/libexec
	export LIBDL_SH_LIBRARY_PATH LIBDL_SH_LIBEXECDIR
	;;
esac
eval "`libdl-sh load`"; dl_load quote	## Load libdl.sh and libquote.sh or die.

cs=`atlocal_get_special_cs` status=0	## Assume success.
for operators in ''							    \
    'sh' 're' 're /' 'ere' 'ere /' 'rs' 'rs /'				    \
    'sh sh' 'sh re' 'sh re /' 'sh ere' 'sh ere /' 'sh rs' 'sh rs /'	    \
    're sh' 're re' 're re /' 're ere' 're ere /' 're rs' 're rs /'	    \
    'ere sh' 'ere re' 'ere re /' 'ere ere' 'ere ere /' 'ere rs' 'ere rs /'  \
    'rs sh' 'rs re' 'rs re /' 'rs ere' 'rs ere /' 'rs rs' 'rs rs /'
do					## Foreach operators list do...
    set shiftee $operators; shift	## Split it into pos. parameters.
    case ${1-sh}${2+,$2} in		## Which operator is 1st?
	sh*)				## 'sh'...
	    eq=${1-sh};	unset de;;
	re,?)				## 're' DELIMITER...
	    eq=$1	de=$2	;;
	re*)				## 're'...
	    eq=$1;	unset de;;
	ere,?)				## 'ere' DELIMITER...
	    eq=$1	de=$2	;;
	ere*)				## 'ere'...
	    eq=$1;	unset de;;
	rs,?)				## 'rs' DELIMITER...
	    eq=$1	de=$2	;;
	rs*)				## 'rs'...
	    eq=$1	de=/	;;
	*)  continue
	    ;;
    esac; eval "set shiftee `atlocal_set_special_cs "$cs${de-}"`; shift"
					## Split special characters into
					## positional parameters.
    for quotee in '' ${1+"$@"} "em^bed${DS}ded" "em${de-}bed${NL}ded"; do
					## Foreach quotee do...
	quoted=`quote $operators "$quotee"`
	atlocal_trace_with_output "$quoted" quote $operators "$quotee"
					## Quote it, then trace invocation
					## with result.
	set shiftee $operators; shift	## Split operators list into posi-
					## tional parameters.
	case ${3+3}${1-sh}${2+,$2} in	## Is it atchable with equivalent?
	    $eq${de+,$de})		## Yes: Match succeeded?
		test "_$quoted" = "_`quote_$eq "$quotee" ${de+"$de"}`" || {
		    status=1; echo '!='	## No, fail test and log failure!
		}
		;;
	esac
    done
done; exit $status			## Indicate success or failure.
], 0, [dnl				## And the winner is...
