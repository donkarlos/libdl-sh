eval "`libdl-sh load`"; dl_load quote	## Load libdl.sh and libquote.sh or die.

echoable() {
    local ifs string; ifs=$IFS; IFS=,; string=$*; IFS=$ifs; sed '
	:N'"$NL"'$!{;N;bN'"$NL"'}
	s/'"$BS$NL"'/<NL>/g;s/'"$CR"'/<CR>/g;s/	/<HT>/g;s/ /<SP>/g
    ' <<EOF
$string
EOF
    IFS=$ifs
}

status=0				## Assume success.
sx1=':N'"$NL"'$!{;N;bN'"$NL"'};s/^/%/;:0'"$NL"'s/%\(.\)/\1'"$BS$NL"'%/;t0'"$NL"'s/%$//'
sx2=':N'"$NL"'$!{;N;s/'"$BS$NL"'//2;tN'"$NL"'bN'"$NL"'}'
sx3=':N'"$NL"'$!{;N;bN'"$NL"'};s/^'"$BS$NL"'/'"$SQ"'&'"$SQ"' %/;t0'"$NL"':0'"$NL"'s/%\('"$LB$quote_cs_sh~$RB"'\)/'"$BS$BS"'\1 %/;t0'"$NL"'s/%\(.\)/\1 %/;t0'"$NL"'s/ %//'

cs=`sed "$sx1" <<EOF | LC_COLLATE=C sort -u | sed "$sx2"
$quote_cs_sh~$quote_cs_bre^$DS$quote_cs_ere$quote_cs_brs
EOF`; for operators in ''						    \
    'sh' 're' 're /' 'ere' 'ere /' 'rs' 'rs /'				    \
    'sh sh' 'sh re' 'sh re /' 'sh ere' 'sh ere /' 'sh rs' 'sh rs /'	    \
    're sh' 're re' 're re /' 're ere' 're ere /' 're rs' 're rs /'	    \
    'ere sh' 'ere re' 'ere re /' 'ere ere' 'ere ere /' 'ere rs' 'ere rs /'  \
    'rs sh' 'rs re' 'rs re /' 'rs ere' 'rs ere /' 'rs rs' 'rs rs /'
do					## Foreach operators list do...
    set shiftee $operators; shift	## Split it into pos. parameters.
    case ${1-sh}${2+,$2} in		## Which operator is 1st?
	sh*)				## "sh"...
	    eq=${1-sh};	unset de;;
	re,?)				## "re" DELIM...
	    eq=$1	de=$2	;;
	re*)				## "re"...
	    eq=$1;	unset de;;
	ere,?)				## "ere" DELIM...
	    eq=$1	de=$2	;;
	ere*)				## "ere"...
	    eq=$1;	unset de;;
	rs,?)				## "rs" DELIM...
	    eq=$1	de=$2	;;
	rs*)				## "rs"...
	    eq=$1	de=/	;;
	*)  continue
	    ;;
    esac; eval "set shiftee `sed "$sx3" <<EOF
$cs${de-}
EOF`"					## Split special characters into
    shift				## positional parameters.
    for quotee in '' ${1+"$@"} "em^bed${DS}ded" "em${de-}bed${NL}ded"; do
					## Foreach quotee do...
	quoted=`quote $operators "$quotee"`
					## Quote quotee.
	equotee=`echoable $operators "$quotee"`
					## Transliterate quotee.
	equoted=`echoable "$quoted"`	## Transliterate quoted.
	printf 'quote(%s)=>%s\n' "$equotee" "$equoted"
	set shiftee $operators; shift
	case ${3+3}${1-sh}${2+,$2} in
	    $eq${de+,$de})
		test "_$quoted" = "_`quote_$eq "$quotee" ${de+"$de"}`" || {
		    status=1; echo '!='
		}
		;;
	esac
    done
done; exit $status			## Indicate success or failure.
